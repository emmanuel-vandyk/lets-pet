{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/test-endpoint.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/debug-endpoint.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "src/test-endpoint.ts"
    },
    {
      "src": "/test/(.*)",
      "dest": "src/test-endpoint.ts"
    },
    {
      "src": "/debug",
      "dest": "src/debug-endpoint.ts"
    },
    {
      "src": "/debug/(.*)",
      "dest": "src/debug-endpoint.ts"
    },
    {
      "src": "/api/docs/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/main.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/test-endpoint.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "src/debug-endpoint.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}
